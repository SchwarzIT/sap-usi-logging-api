      "importing": "^I_.+$",
      "returning": "^R_.+$",
      "changing": "^C_.+$",
      "exporting": "^E_.+$"
    },
    "mix_returning": {
      "exclude": [],
      "severity": "Error"
    },
    "modify_only_own_db_tables": {
      "exclude": [],
