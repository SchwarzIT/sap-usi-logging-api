      "intf": "^/USI/IF_BAL_",
      "prog": "^/USI/BAL_",
      "fugr": "^/USI/BAL_",
      "tabl": "^/USI/BAL_",
      "ttyp": "^/USI/BAL_",
      "dtel": "^/USI/BAL_",
      "doma": "^/USI/BAL_",
      "msag": "^/USI/BAL",
      "tran": "^/USI/BAL_",
      "enqu": "^/USI/E_BAL_",
      "auth": "^/USI/BAL_",
      "pinf": "^/USI/BAL_",
      "idoc": "^/USI/BAL_",
      "xslt": "^/USI/BAL_",
      "ssfo": "^/USI/BAL_",
      "ssst": "^/USI/BAL_",
      "shlp": "^/USI/BAL_"
