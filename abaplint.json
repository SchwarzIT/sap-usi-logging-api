    "exit_or_check": false,
    "exporting": {
      "exclude": [],
      "severity": "Error"
    },
    "forbidden_identifier": {
      "exclude": [],
      "severity": "Error",
      "check": []
    },
    "forbidden_pseudo_and_pragma": {
      "exclude": [],
      "severity": "Error",
      "pseudo": [],
      "pragmas": [],
      "ignoreGlobalClassDefinition": false,
      "ignoreGlobalInterface": false
    },
    "forbidden_void_type": {
      "exclude": [],
      "severity": "Error",
      "check": []
