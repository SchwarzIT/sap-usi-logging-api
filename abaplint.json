      "exclude": [],
      "severity": "Error"
    },
    "names_no_dash": {
      "exclude": [],
      "severity": "Error"
    },
    "nesting": {
      "exclude": [],
      "severity": "Error",
      "depth": 5
    },
    "newline_between_methods": {
      "exclude": [],
      "severity": "Error",
      "count": 3,
      "logic": "less"
    },
    "no_aliases": false,
    "no_chained_assignment": {
      "exclude": [],
      "severity": "Error"
