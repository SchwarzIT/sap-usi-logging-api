      "kernelCall": true,
      "systemCall": true,
      "insertReport": true,
      "generateDynpro": true,
      "generateReport": true,
      "generateSubroutine": true,
      "deleteReport": true,
      "deleteTextpool": true,
      "deleteDynpro": true,
      "exportDynpro": true,
      "dynamicSQL": false
