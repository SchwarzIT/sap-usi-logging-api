      "exception": "^LCX_.+$",
      "test": "^LCL_.+$"
    },
    "local_testclass_consistency": {
      "exclude": [],
      "severity": "Error"
    },
    "local_variable_names": {
      "exclude": [],
      "severity": "Error",
      "patternKind": "required",
      "ignoreNames": [],
      "ignorePatterns": [],
      "expectedData": "^.+$",
      "expectedConstant": "^.+$",
      "expectedFS": "^<.+>$"
    },
    "main_file_contents": {
      "exclude": [],
      "severity": "Error"
    },
    "many_parentheses": {
      "exclude": [],
      "severity": "Error"
    },
